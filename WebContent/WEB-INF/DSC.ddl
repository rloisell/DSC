create table `Budget` (`BudgetId` int(11) not null auto_increment, `budgetDescription` char(10) not null, primary key (`BudgetId`)) comment='Currently  there are 2 budget types, Expense = 0 and Capital = 1. Another table was created to allow this category to get more granular with time. The selection of a row in this table allows the application to determine if there is a corresponding Project_Activity or Expense_Activity record.' type=InnoDB CHARACTER SET UTF8;
