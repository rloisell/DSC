create table `Activity` (`activityID` int(11) default 0 not null, `Calendardate` date not null, `description` varchar(255), `startTime` timestamp, `endTime` timestamp, `plannedDuration` time, `actualDuration` int(11) not null, `UserempId` int(11) not null, `BudgetBudgetId` int(11) not null, `CategorycategoryID` int(11) not null, primary key (`activityID`), unique index (`activityID`)) type=InnoDB CHARACTER SET UTF8;
