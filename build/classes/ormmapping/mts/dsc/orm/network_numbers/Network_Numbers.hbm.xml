<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="mts.dsc.orm.network_numbers.Network_Numbers" table="`Network_Numbers`" lazy="false">
		<id name="networkNumber" column="`networkNumber`" type="integer">
			<generator class="assigned">
			</generator>
		</id>
		<property name="description" column="`description`" type="string" length="255" not-null="false" lazy="false"/>
		<set name="Project_Activity" lazy="true" cascade="save-update,lock" inverse="true">
			<key column="`Network_NumbersnetworkNumber`" not-null="true"/>
			<one-to-many class="mts.dsc.orm.project_activity.Project_Activity"/>
		</set>
	</class>
</hibernate-mapping>
